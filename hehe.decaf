class Animal
{
    int eat()
    {
        Print("I am Eating");
    }
    int name()
    {
        Print("I am an animal.");
    }
}

class Chicken extends Animal
{
    int name()
    {
        Print("I am chicken.");
    }
}

class Cow extends Animal
{
    int name()
    {
        Print("I am a cow.");
    }
}

int main(int argc, string argv)
{
    Animal a;
    Animal b;
    a = New(Chicken);
    b = New(Cow);
    a.name();
    a.eat();
    b.name();
    b.eat();
    return 0;
}
